t', 'House', 'Forest', 'Children', 'Hunting', 'Dog', 'Horse', 'Forest', 'Landscape', 'Traveling', 'Cattle', 'Dog', 'Horse', 'Forest', 'Road', 'Wagon', 'Horse', 'Forest', 'Road', 'Wagon', 'Forest', 'Landscape', 'Ship', 'Peasants', 'Traveling', 'Cattle', 'Horse', 'Forest', 'Road', 'Wagon', 'Horse', 'Forest', 'Cart', 'Road', 'Traveling', 'Cattle', 'Horse', 'Swine', 'Forest', 'Stream', 'Burghers', 'Peasants', 'Road', 'Wagon', 'Resting', 'Traveling', 'Dog', 'Horse', 'Road', 'Wagon', 'Forest'])).sort();
      
      buttonDiv.append("<button class='btn facet active' data-filter='all'>show all</button>");
      
      for (i in facets) {
          var slug = facets[i].replace(/\s/g, "-").replace(/,/g, "").replace(/\//g, "-");
          buttonDiv.append(`<button class='btn facet' data-filter='${slug.toLowerCase()}'>${facets[i]}</button>`);
        }

        $(document).ready(function(){
          $(".facet").click(function(){
            var filterValue = $(this).attr('data-filter');
            $('.facet').removeClass('active');
            $(this).addClass('active');
            if(filterValue == 'all') { $('.all').show(); }
            else { $('.all').hide(); $(`.${filterValue}`).show(); }
          });
        });
    
  });
</script>

