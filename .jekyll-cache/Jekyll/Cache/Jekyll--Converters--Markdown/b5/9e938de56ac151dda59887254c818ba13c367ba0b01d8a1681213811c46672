        var facets = Array.from(new Set(['', '', 'Germany', 'Austria', 'France', '', '', '', '', '', '', '', '', 'England', '', 'Austria', 'Netherlands', '', 'Austria', '', 'Austria', '', 'England', 'United States', 'Austria', 'Belgium', '', '', 'England', '', 'Belgium', '', 'Netherlands', '', 'Netherlands', 'France', '', '', 'United States', 'France', 'Czech Republic', 'Germany', '', 'Austria', '', '', 'Italy', 'Germany', '', 'Belgium', 'England', 'Czech Republic', 'England', 'United States', 'England', 'Belgium', '', 'United States', '', 'England', '', 'France', '', 'Belgium', 'Germany', 'England', 'France', 'Netherlands', 'Germany', 'France', '', 'Norway', 'Netherlands', '', 'Sweden', '', '', 'Belgium', 'Germany', '', 'France', 'Germany', 'Germany', 'France', 'France', 'France', 'France', '', '', 'France', '', '', '', 'England', 'Germany', 'Germany', '', 'Austria', '', '', 'England', 'Austria', '', 'Belgium', 'France', '', 'Germany', 'France', '', 'Netherlands', 'England', 'Belgium', 'Switzerland', '', 'Germany', '', 'Germany', '', 'United States', 'Austria', '', 'Germany', 'France', '', 'Austria', 'England', 'Belgium', 'Austria', '', '', 'England', '', 'Austria', 'United States', '', '', 'Austria', 'Netherlands', 'Germany', 'Belgium', '', 'Netherlands', '', 'England', '', 'Austria', 'United States', '', '', '', 'Germany', '', 'Austria', 'Germany', 'England', '', 'Italy', 'Austria', '', '', '', 'England', 'Italy', '', '', '', 'Austria', '', 'Germany', '', 'France', '', 'Netherlands', 'Austria', 'Hungary', '', 'Austria', 'United States', 'England', '', '', 'Netherlands', 'Spain', '', 'Austria', 'France', '', 'Austria', 'United States', 'Belgium', '', 'Spain', '', 'United States', 'Belgium', '', 'Germany', 'Netherlands', 'Italy', '', 'Belgium', '', '', 'Italy', 'England', 'Belgium'])).sort();
      
      buttonDiv.append("<button class='btn facet active' data-filter='all'>show all</button>");
      
      for (i in facets) {
          var slug = facets[i].replace(/\s/g, "-").replace(/,/g, "").replace(/\//g, "-");
          buttonDiv.append(`<button class='btn facet' data-filter='${slug.toLowerCase()}'>${facets[i]}</button>`);
        }

        $(document).ready(function(){
          $(".facet").click(function(){
            var filterValue = $(this).attr('data-filter');
            $('.facet').removeClass('active');
            $(this).addClass('active');
            if(filterValue == 'all') { $('.all').show(); }
            else { $('.all').hide(); $(`.${filterValue}`).show(); }
          });
        });
    
  });
</script>

